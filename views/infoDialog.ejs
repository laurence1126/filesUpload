<script>
$.sendConfirm({
    title: 'Information',
    withNoMinWidth: true,
    width: '400px',
    content: '<%- content %>',
    button: {
        confirm: 'Confirm',
        cancel: 'Cancel'
    },
    onConfirm: function() {
        $("#send-value").submit();
        console.log('点击确定！');
    },
    onCancel: function() {
        console.log('点击取消！');
    },
    onClose: function() {
        console.log('点击关闭！');
    }
});
</script>